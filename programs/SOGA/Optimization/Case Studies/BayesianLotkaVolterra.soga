array[3] x;
array[3] y;

xcurr = 20.;
ycurr = 20.;

alpha = gm([1.], [1.5], [0.01]);
beta = gm([1.], [0.1], [0.01]);

for i in range(3){

    prod = xcurr * ycurr;
    prod1 = beta * prod;
    prod2 = alpha * xcurr;

    x[i] = xcurr + 0.1 * prod2 - 0.1 * prod1;
    y[i] = 0.7 * ycurr + 0.1 * prod1;

    xcurr = x[i];
    ycurr = y[i];

} end for; 

